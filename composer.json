{
    "name": "oggettoweb/parallels",
    "version": "1.0.0",
    "type": "magento-module",
    "license": "OSL-3.0",
    "homepage": "https://github.com/dankocherga/Magento-Parallels",
    "description": "Oggetto Parallels extension for run background tasks",
    "authors": [
        {
            "name": "Dan Kocherga",
            "email": "dan@oggettoweb.com"
        }
    ],
    "require": {
        "magento-hackathon/magento-composer-installer": "*",
        "phpunit/phpunit": "3.7.*"
    },
    "extra": {
        "map": [
            [".parallels/", ".parallels/"],
            ["app/etc/modules/Oggetto_Parallels.xml", "app/etc/modules/Oggetto_Parallels.xml"],
            ["app/code/local/Oggetto/Parallels/", "app/code/local/Oggetto/Parallels/"]
        ]
    }
}
